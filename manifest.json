{
  "manifest_version": 3,
  "name": "My jQuery Extension",
  "version": "1.0",
  "permissions": ["activeTab", "tabs", "storage"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "index.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["jquery-3.7.1.min.js", "content.js"]
    }
  ]
}
